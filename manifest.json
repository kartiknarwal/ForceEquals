{
    "manifest_version": 3,
    "name": "LinkedIn Profile Enhancer",
    "version": "1.0",
    "description": "Displays match score and account status on LinkedIn profiles.",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["*://*.linkedin.com/in/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  